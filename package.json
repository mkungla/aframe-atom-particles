{
  "name": "atom-aframe",
  "productName": "A-Frame",
  "description": "Atom A-Frame Support",
  "main": "./src/index",
  "version": "0.1.1",
  "license": "MIT",
  "keywords": [
    "aframe",
    "vr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mkungla/atom-aframe.git"
  },
  "bugs": {
    "url": "https://github.com/mkungla/atom-aframe/issues"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "lint": "eslint ."
  },
  "devDependencies": {
    "aframe": "^0.7.1",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "atomtest": true,
      "jasmine": true,
      "mocha": true
    },
    "globals": {
      "atom": true
    }
  },
  "dependencies": {
    "semver": "^5.4.1"
  },
  "atomTestRunner": "./tests/runner",
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "aframeStatusBar"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "aframeAutocompleteProvider"
      }
    }
  },
  "configSchema": {
    "global": {
      "type": "object",
      "order": 1,
      "title": "Global Atom A-Frame package configuration",
      "properties": {
        "notifOnActivationFailure": {
          "title": "Warn when this atom package is unable to activate",
          "description": "Show warning this atom package can not be activate (e.g to old Atom)",
          "type": "boolean",
          "default": true,
          "order": 1
        }
      }
    }
  }
}
